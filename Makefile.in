PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir = @PUBRULE@
pubdir = @prefix@
srcdir = @srcdir@
applib = FDL
appname = @APPNAME@
TAR = tar
GZIP_ENV = --best

export PACKAGE utildir pubdir applib appname

include $(utildir)/PubRule
publish: $(pubdir)

clean:
	/bin/rm -f *.*~ config.* Makefile configure $(appname)_init.php info.xml $(PACKAGE)-*webinst
	/bin/rm -fr autom4te.cache
